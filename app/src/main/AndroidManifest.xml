<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="winter.zxb.smilesb101.coderhome">

    <uses-permission-sdk-23 android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app"
        android:supportsRtl="true"
        android:theme="@style/DayTheme">
        <activity
            android:name=".View.Activitys.WelcomeActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".View.Activitys.MainActivity"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".View.Activitys.GanioDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".View.Activitys.ZhiHuDetailsActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".View.Activitys.SkinActivity"
            android:label="@string/title_activity_skin"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".View.Activitys.AboutProgrammAvtivity"
                  android:theme="@style/AppTheme.NoActionBar"
            android:label="关于我们">
        </activity>
    </application>

</manifest>
